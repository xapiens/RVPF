@ECHO OFF

REM $Id: rvpf-env.cmd 3096 2016-07-13 17:00:03Z SFB $

CALL setup\rvpf
IF NOT "%ERRORLEVEL%"=="0" EXIT /B %ERRORLEVEL%

SET RVPF_BIN=%RVPF_SUB%\bin
SET RVPF_CONFIG=%RVPF_SUB%\config
SET RVPF_DATA=%RVPF_SUB%\data
SET RVPF_LIB=%RVPF_SUB%\lib
SET RVPF_LOG=%RVPF_SUB%\log
SET RVPF_RUN=%RVPF_SUB%\script
SET RVPF_SCRIPT=%RVPF_SUB%\run
SET RVPF_SHARE=%RVPF_SUB%\lib
SET RVPF_SHARE_JAVA=%RVPF_SUB%\lib
SET RVPF_TMP=%RVPF_SUB%\tmp

SET RVPF_CORE_BIN=%RVPF_HOME%\core\bin
SET RVPF_CORE_CONFIG=%RVPF_HOME%\core\config
SET RVPF_CORE_LIB=%RVPF_HOME%\core\lib
SET RVPF_CORE_SCRIPT=%RVPF_HOME%\core\script
SET RVPF_CORE_SHARE=%RVPF_HOME%\core\lib
SET RVPF_CORE_SHARE_JAVA=%RVPF_HOME%\core\lib

IF NOT "%1"=="" (
    SET RVPF_TESTS_CONFIG=%RVPF_SUB%\tests\config
    SET RVPF_TESTS_CLASSES=%RVPF_SUB%\tests\classes
    SET RVPF_TESTS_DATA=%RVPF_SUB%\tests\data
)

EXIT /B 0

REM End.
